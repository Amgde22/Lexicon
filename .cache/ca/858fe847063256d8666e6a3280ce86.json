line":242,"column":19},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":183,"column":4}},{"generated":{"line":242,"column":20},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":183,"column":11},"name":"off"},{"generated":{"line":242,"column":23},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":183,"column":4}},{"generated":{"line":242,"column":24},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":183,"column":15},"name":"event"},{"generated":{"line":242,"column":29},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":183,"column":4}},{"generated":{"line":242,"column":31},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":183,"column":22},"name":"effect"},{"generated":{"line":242,"column":37},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":183,"column":4}},{"generated":{"line":242,"column":39},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":182,"column":28}},{"generated":{"line":242,"column":40},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":185,"column":4}},{"generated":{"line":243,"column":4},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":186,"column":4}},{"generated":{"line":244,"column":4},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":187,"column":4}},{"generated":{"line":245,"column":4},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":188,"column":4}},{"generated":{"line":246,"column":4},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":189,"column":4}},{"generated":{"line":247,"column":4},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":190,"column":4}},{"generated":{"line":249,"column":2},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":191,"column":3}},{"generated":{"line":250,"column":0},"source":"jscripts/components/buffs/effectsHandler.js","original":{"line":193,"column":1}}],"sources":{"jscripts/components/buffs/effectsHandler.js":"import effects from \"../buffsManager\"\nimport { renderEffect } from \"../functions\"\nimport pubsub from \"../pubsub.js\"\n\nexport const renderEffect2 = renderEffect\nexport {renderEffect} from \"../functions\"\n// add effect\n// stacking\n  export function applyEffectHandler(effect , entity) {\n    const type_of_effect = effect.constructor.generatedEffect\n    const effect_list = entity[type_of_effect]\n      const effectName = effect.name\n      const effectDuration = effect.duration\n      const effectUses = effect.uses\n      \n  // link 'em\n      effect.hostingEntity = entity\n    \n    if(effect_list[effectName]){\n  // reinforce effect if exists\n      reinforceEffect( effect_list[effectName] , effectDuration , effectUses)\n      effect.alreadyAplied = true\n    }\n\n    else{\n  // add effect if it doesn't exist\n      effect_list[effectName] = effect\n    }\n\n      renderEffect(entity , effect_list[effectName])\n    }\n\n  export function applyStackingEffectHandler(effect , entity){\n    const type_of_effect = effect.constructor.generatedEffect\n    const effect_list = entity[type_of_effect]\n      const effectName = effect.name\n\n    if(effect_list[effectName]){\n      for(let i = 0 ; i < 99 ; i++){\n        if(effect_list[effectName + i] != undefined){continue;}\n          effect.name = effectName + i\n          break;\n      }\n    }\n  // add effect with original / newname\n    effect_list[effectName] = effect\n    renderEffect(entity , effect)\n\n  }\n\n// non stacking\n\n\n// adding effects to list\n // deal damae\n  export function addTodealDamageEffects(effect) {\n    effect.hostingEntity.dealDamageEffects[effect.name] = effect\n  }\n  export function removeFromdealDamageEffects(effect) {\n    delete effect.hostingEntity.dealDamageEffects[effect.name]\n  }\n\n// recieve damage\n  export function addTorecieveDamageEffects(effect) {\n    effect.hostingEntity.recieveDamageEffects[effect.name] = effect\n  }\n  export function removeFromrecieveDamageEffects(effect) {\n    delete effect.hostingEntity.recieveDamageEffects[effect.name]\n  }\n\n// block effects\n  export function addToblockEffects(effect) {\n    effect.hostingEntity.blockEffects[effect.name] = effect\n  }\n  export function removeFromblockEffects(effect) {\n    delete effect.hostingEntity.blockEffects[effect.name]\n  }\n\n// card effects\n  export function addTocardEffects(effect , numOdList) {\n  const cardList = getListBasedOnNumber(numOdList)\n    effect.hostingEntity.cardEffects[cardList][effect.name] = effect\n  }\n  export function removeFromcardEffects(effect , numOdList) {\n  const cardList = getListBasedOnNumber(numOdList)\n    delete effect.hostingEntity.cardEffects[cardList][effect.name]\n  }\n    function getListBasedOnNumber(number) {\n        if(number === 1){return \"playEffects\"}\n        if(number === 2){return \"drawEffects\"}\n        if(number === 3){return \"exaustEffects\"}\n        if(number === 4){return \"discardEffects\"}\n    }\n\n// turn effects\n  export function addToturnEffects(effect) {\n    effect.hostingEntity.turnEffects[effect.name] = effect\n  }\n  export function removeFromturnEffects(effect) {\n    delete effect.hostingEntity.turnEffects[effect.name]\n  }\n\n// /add effect\n\n\n// effect timer\n  export function halfEffectDuration(effect) {\n    if(typeof(effect.duration) !== \"number\"){ throw new Error(\"inf effect want to be hallfed\" , effect) ; return}\n    effect.duration = Math.floor(effect.duration / 2)\n    if(effect.duration <= 0){effect.remove()}\n    renderEffect(effect.hostingEntity , effect)\n  }\n\n  export function reduceEffectDuration(effect) {\n    if(typeof(effect.duration) !== \"number\"){return}\n    effect.duration--\n    if(effect.duration <= 0){effect.remove()}\n    renderEffect(effect.hostingEntity , effect)\n\n  }\n\n  export function reduceEffectUses(effect) {\n    if(typeof(effect.uses) == \"string\"){return}\n    effect.uses--\n    if(effect.uses <= 0){effect.remove()}\n    renderEffect(effect.hostingEntity , effect)\n\n  }\n// /effect timer\n\n// reinforce effect\nfunction reinforceEffect(exactEffect , bonusDuration , bonusUses) {\n\n  reinforceDuration(exactEffect , bonusDuration)\n  reinforceUses(exactEffect , bonusUses)\n\n}\n\n      function reinforceDuration(exactEffect ,bonusDuration ) {\n        if(exactEffect.duration === \"inf\"){return}\n\n        else if(bonusDuration === \"inf\"){\n          exactEffect.duration = \"inf\"\n          return\n        }\n\n        else if(typeof(bonusDuration) === \"number\"){\n          exactEffect.duration += bonusDuration\n          return\n        }\n// fall back\n        else{\n          console.log( bonusDuration,\" added to \" , exactEffect );  \n          alert(\"effectsHandler : type of effect duration not intended , chack consle\")\n        }\n \n      }\n\n\n      function reinforceUses(exactEffect ,bonusUses ) {\n        if(exactEffect.uses === \"inf\"){return}\n\n        else if(bonusUses == \"inf\"){\n          exactEffect.uses = \"inf\"\n          return\n        }\n\n        else if(typeof(bonusUses) == \"number\"){\n          exactEffect.uses += bonusUses\n          return\n        }\n// fall back\n        else{alert(\"effectsHandler : type of effect duration not intended , chack consle\")\n            console.log( bonusUses,\" added to \" , exactEffect );  \n        }\n      }\n// /reinforce effect\n\n// force remove\nexport function force_remove(effect){\n  const events = pubsub.events\n  for(const event in events){\n    pubsub.off(event ,effect )\n\n    // for(let i = 0 ; i < events[event].length ; i++){\n    //   const index = events[event].findIndex(e => e === effect)\n    //   if (index > 0) {\n    //     events[event].splice(i,1)\n    //   }\n    // }\n  }\n\n}\n"},"lineCount":null}},"error":null,"hash":"0b444b6bfc7d6d768b8b7026d5752d27","cacheData":{"env":{}}}
